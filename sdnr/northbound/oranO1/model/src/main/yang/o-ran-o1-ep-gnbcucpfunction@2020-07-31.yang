module o-ran-o1-ep-gnbcucpfunction {

  yang-version 1.1;
  namespace "urn:o-ran:o1-ep-gnbcucpfunction:1";
  prefix "o-ran-o1-ep-cucp";

  import _3gpp-common-managed-element { prefix me3gpp; }
  import _3gpp-common-top { prefix top3gpp; }
  import _3gpp-nr-nrm-gnbcucpfunction { prefix gnbcucp3gpp; }
  import o-ran-o1-ep { prefix o-ran-o1-ep; }

  organization "O-RAN Alliance";
  contact "www.o-ran.org";
  description
    "Augments the 3GPP GNBCUCPFunction with the endpoint IOCs that are part of the O-RAN O1-Interface Model.
    

  revision 2020-11-01 {
    description
      "version 1.0.0
       Initial version.";
    reference
      "O-RAN Operations and Maintenance Architecture
       O-RAN.WG1.OAM-Architecture-v03.00
       3GPP TS 28.541
         Management and orchestration; 5G Network Resource Model (NRM)";
  }

  identity o-ran-managed-function-type-id {
    description
      "Identifies an O-RAN managed function. The description of the O-RAN
       managed function type identifier MUST indicate whether or not the O-RAN
       managed function type identifier is abstract.
       An abstract O-RAN managed function type identifier is used as a base for
       other O-RAN managed function ids and will not be used as a value.";
  }

  identity o-ru {
    base o-ran-managed-function-type-id;
    description
      "O-RU: O-RAN Radio Unit:
       A logical node hosting Low-PHY layer and RF processing based on a lower
       layer functional split. This is similar to 3GPPâ€™s 'TRP' or 'RRH' but
       more specific in including the Low-PHY layer
       (FFT/iFFT, PRACH extraction).";
  }

  identity o-du {
    base o-ran-managed-function-type-id;
    description
      "O-DU: O-RAN Distributed Unit:
       A logical node hosting RLC/MAC/High-PHY layers based on a lower layer
       functional split.";
  }

  // [sko] an o-cu is the combination of the managed functions o-cu-up and
  //       o-cu-cp, therefore it is not needed as extra identity.
  // identity o-cu {
  //   base o-ran-managed-function-type-id;
  //   description
  //     "O-CU: O-RAN Central Unit:
  //      A logical node hosting RRC, SDAP and PDCP protocols.";
  // }

  identity o-cu-up {
    base o-ran-managed-function-type-id;
    description
      "O-CU-UP: O-RAN Central Unit - User Plane:
       A logical node hosting the user plane part of the PDCP protocol and the
       SDAP protocol.";
  }

  identity o-cu-cp {
    base o-ran-managed-function-type-id;
    description
      "O-CU-CP: O-RAN Central Unit - Control Plane:
       A logical node hosting the RRC and the control plane part of the
       PDCP protocol.";
  }

  identity near-rt-ric {
    base o-ran-managed-function-type-id;
    description
      "near-RT RIC: O-RAN near-real-time RAN Intelligent Controller:
       A logical function that enables near-real-time control and optimization
       of O-RAN elements and resources via fine-grained data collection and
       actions over E2 interface.";
  }

  // [sko] non-rt-ric does not provide an Netconf/YANG O1 interface and is not
  //       an O-RAN managed function.
  // identity non-rt-ric {
  //   base o-ran-managed-function-type-id;
  //   description
  //     "non-RT RIC: O-RAN non-real-time RAN Intelligent Controller:
  //      A logical function that enables non-real-time control and optimization of
  //      RAN elements and resources, AI/ML workflow including model training and
  //      updates, and policy-based guidance of applications/features in
  //      near-RT RIC.";
  // }

  // [sko] o-cloud does not provide an Netconf/YANG O1 interface and is not
